>>> +static int qcom_battmgr_set_charge_start_threshold(struct qcom_battmgr *battmgr, int start_soc)
>>> +{
>>> +Â Â Â  u32 target_soc, delta_soc;
>>> +Â Â Â  int ret;
>>> +
>>> +Â Â Â  if (start_soc < CHARGE_CTRL_START_THR_MIN ||
>>> +Â Â Â Â Â Â Â  start_soc > CHARGE_CTRL_START_THR_MAX) {
>>> +Â Â Â Â Â Â Â  dev_err(battmgr->dev, "charge control start threshold exceed range: [%u - %u]\n",
>>> +Â Â Â Â Â Â Â Â Â Â Â  CHARGE_CTRL_START_THR_MIN, CHARGE_CTRL_START_THR_MAX);
>>> +Â Â Â Â Â Â Â  return -EINVAL;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  /*
>>> +Â Â Â Â  * If the new start threshold is larger than the old end threshold,
>>> +Â Â Â Â  * move the end threshold one step (DELTA_SOC) after the new start
>>> +Â Â Â Â  * threshold.
>>> +Â Â Â Â  */
>>> +Â Â Â  if (start_soc > battmgr->info.charge_ctrl_end) {
>>> +Â Â Â Â Â Â Â  target_soc = start_soc + CHARGE_CTRL_DELTA_SOC;
>>> +Â Â Â Â Â Â Â  target_soc = min_t(u32, target_soc, CHARGE_CTRL_END_THR_MAX);
>>> +Â Â Â Â Â Â Â  delta_soc = target_soc - start_soc;
>>> +Â Â Â Â Â Â Â  delta_soc = min_t(u32, delta_soc, CHARGE_CTRL_DELTA_SOC);
>>> +Â Â Â  } else {
>>> +Â Â Â Â Â Â Â  target_soc =Â  battmgr->info.charge_ctrl_end;
>>> +Â Â Â Â Â Â Â  delta_soc = battmgr->info.charge_ctrl_end - start_soc;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  mutex_lock(&battmgr->lock);
>>> +Â Â Â  ret = qcom_battmgr_set_charge_control(battmgr, target_soc, delta_soc);
>>> +Â Â Â  mutex_unlock(&battmgr->lock);
>>> +Â Â Â  if (!ret) {
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_start = start_soc;
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_end = target_soc;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  return 0;
>>> +}
>>> +
>>> +static int qcom_battmgr_set_charge_end_threshold(struct qcom_battmgr *battmgr, int end_soc)
>>> +{
>>> +Â Â Â  u32 delta_soc = CHARGE_CTRL_DELTA_SOC;
>>> +Â Â Â  int ret;
>>> +
>>> +Â Â Â  if (end_soc < CHARGE_CTRL_END_THR_MIN ||
>>> +Â Â Â Â Â Â Â  end_soc > CHARGE_CTRL_END_THR_MAX) {
>>> +Â Â Â Â Â Â Â  dev_err(battmgr->dev, "charge control end threshold exceed range: [%u - %u]\n",
>>> +Â Â Â Â Â Â Â Â Â Â Â  CHARGE_CTRL_END_THR_MIN, CHARGE_CTRL_END_THR_MAX);
>>> +Â Â Â Â Â Â Â  return -EINVAL;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  if (battmgr->info.charge_ctrl_start && end_soc > battmgr->info.charge_ctrl_start)
>>> +Â Â Â Â Â Â Â  delta_soc = end_soc - battmgr->info.charge_ctrl_start;
>>> +
>>> +Â Â Â  mutex_lock(&battmgr->lock);
>>> +Â Â Â  ret = qcom_battmgr_set_charge_control(battmgr, end_soc, delta_soc);
>>> +Â Â Â  mutex_unlock(&battmgr->lock);
>>> +Â Â Â  if (!ret) {
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_start = end_soc - delta_soc;
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_end = end_soc;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  return 0;
>>> +}
>>> +
>>> +static int qcom_battmgr_charge_control_thresholds_init(struct qcom_battmgr *battmgr)
>>> +{
>>> +Â Â Â  int ret;
>>> +Â Â Â  u8 en, end_soc, start_soc, delta_soc;
>>> +
>>> +Â Â Â  ret = nvmem_cell_read_u8(battmgr->dev->parent, "charge_limit_en", &en);
>>> +Â Â Â  if (!ret && en != 0) {
>>> +Â Â Â Â Â Â Â  ret = nvmem_cell_read_u8(battmgr->dev->parent, "charge_limit_end", &end_soc);
>>> +Â Â Â Â Â Â Â  if (ret < 0)
>>> +Â Â Â Â Â Â Â Â Â Â Â  return ret;
>>> +
>>> +Â Â Â Â Â Â Â  ret = nvmem_cell_read_u8(battmgr->dev->parent, "charge_limit_delta", &delta_soc);
>>> +Â Â Â Â Â Â Â  if (ret < 0)
>>> +Â Â Â Â Â Â Â Â Â Â Â  return ret;
>>> +
>>> +Â Â Â Â Â Â Â  if (delta_soc >= end_soc)
>>> +Â Â Â Â Â Â Â Â Â Â Â  return -EINVAL;
>>> +
>>> +Â Â Â Â Â Â Â  start_soc = end_soc - delta_soc;
>>> +Â Â Â Â Â Â Â  end_soc = clamp(end_soc, CHARGE_CTRL_END_THR_MIN, CHARGE_CTRL_END_THR_MAX);
>>> +Â Â Â Â Â Â Â  start_soc = clamp(start_soc, CHARGE_CTRL_START_THR_MIN, CHARGE_CTRL_START_THR_MAX);
>>> +
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_start = start_soc;
>>> +Â Â Â Â Â Â Â  battmgr->info.charge_ctrl_end = end_soc;
>>> +Â Â Â  }
>>> +
>>> +Â Â Â  return 0;
>>> +}
